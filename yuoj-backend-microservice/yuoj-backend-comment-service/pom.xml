    </parent>

    <artifactId>yuoj-backend-comment-service</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>yuoj-backend-comment-service</name>
    <description>yuoj-backend-comment-service</description>

    <properties>
